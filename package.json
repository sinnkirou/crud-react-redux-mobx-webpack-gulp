{
  "name": "crud-react-redux",
  "sideEffects": false,
  "version": "0.1.0",
  "private": true,
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime",
        {
          "corejs": 2
        }
      ],
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions"
    ],
    "comments": true,
    "env": {
      "test": {
        "presets": [
          "@babel/preset-env"
        ],
        "plugins": [
          "@babel/plugin-transform-modules-commonjs",
          "dynamic-import-node",
          "@babel/plugin-syntax-dynamic-import",
          "@babel/plugin-syntax-import-meta",
          "@babel/plugin-proposal-class-properties",
          "@babel/plugin-proposal-json-strings",
          [
            "@babel/plugin-proposal-decorators",
            {
              "legacy": true
            }
          ],
          "@babel/plugin-proposal-function-sent",
          "@babel/plugin-proposal-export-namespace-from",
          "@babel/plugin-proposal-numeric-separator",
          "@babel/plugin-proposal-throw-expressions"
        ],
        "retainLines": true
      }
    }
  },
  "engines": {
    "node": "8.12.0",
    "npm": "6.4.1"
  },
  "dependencies": {
    "compression": "^1.7.3",
    "connected-react-router": "^4.4.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.16.3",
    "history": "^4.7.2",
    "http-errors": "^1.7.0",
    "lodash": "^4.17.19",
    "moment": "^2.22.2",
    "prop-types": "^15.7.2",
    "pug": "^2.0.3",
    "react": "^16.9.0",
    "react-chat-widget": "^2.1.4-1",
    "react-dom": "^16.9.0",
    "react-loadable": "^5.5.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "uuid": "^3.3.2",
    "websocket": "^1.0.29",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime-corejs2": "^7.4.2",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^8.0.0",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.2.0",
    "css-loader": "^0.28.11",
    "del": "^4.1.0",
    "dotenv": "^6.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.11.0",
    "gulp": "^4.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-load-plugins": "^1.5.0",
    "gulp-nodemon": "^2.4.2",
    "gulp-rename": "^1.4.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "jsdom": "11.11.0",
    "jsdom-global": "3.0.2",
    "mocha": "^5.2.0",
    "nodemon-webpack-plugin": "^4.0.3",
    "nyc": "^12.0.2",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "raf": "^3.4.0",
    "react-hot-loader": "^4.3.11",
    "react-test-renderer": "^16.4.1",
    "redux-mock-store": "^1.5.3",
    "sinon": "^6.1.5",
    "style-loader": "^0.21.0",
    "treer": "^1.0.4",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.1.3",
    "webpack-hot-middleware": "^2.22.3",
    "webpack-merge": "^4.1.4",
    "webpack-node-externals": "^1.7.2"
  },
  "scripts": {
    "start": "webpack --watch --config webpack.dev.js --trace-sync-io",
    "build": "webpack --config webpack.prod.js",
    "lint": "eslint src test --ext js,jsx",
    "lint:fix": "eslint src test --ext js,jsx --fix",
    "test:quick": "cross-env NODE_ENV=test mocha \"test/**/*.spec.js\" --require @babel/register --require test/test_helper.js",
    "test": "cross-env NODE_ENV=test nyc --extension .jsx --reporter=lcov --reporter=text mocha \"test/**/*.spec.js\" -t 10000 --require @babel/register --require test/test_helper.js",
    "heroku-postbuild": "npm run build",
    "treer": "treer -i \"/^.git|node_modules|.nyc_output|.vscode|coverage|dist|crud.log.gz|treerOutput$/\" -e treerOutput",
    "gulp": "gulp",
    "gulp:build": "gulp build_prod",
    "babel-upgrade": "npx babel-upgrade --write --install"
  }
}
